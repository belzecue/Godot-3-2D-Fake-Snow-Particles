; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "FakeSnowParticles",
"language": "GDScript",
"path": "res://fake_snow_particles.gd"
} ]
_global_script_class_icons={
"FakeSnowParticles": ""
}

[application]

config/name="Godot 3 2D Fake Snow Particles"
config/description="A script to simulate snow particles ❄️."
run/main_scene="res://fake_snow_particles.tscn"
config/icon="res://icon.png"
config/version="1.0.0"
config/godot_version="3.x"
config/license="MIT"
config/author="hiulit"
config/banner="examples/fake-snow-particles-example.gif"
config/patreon="https://www.patreon.com/hiulit"
config/ko-fi="https://ko-fi.com/F2F7136ND"
config/buy_me_a_coffee="https://www.buymeacoffee.com/hiulit"
config/paypal="https://www.paypal.com/paypalme/hiulit"
config/repository="https://github.com/hiulit/Godot-3-2D-Fake-Snow-Particles"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=640
window/size/test_height=360
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
